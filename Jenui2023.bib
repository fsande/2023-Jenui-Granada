@Article{Aldinucci:2003:AES,
  author        = {Marco Aldinucci and Marco Danelutto and P. Teti},
  title         = {An advanced environment supporting structured parallel
                  programming in {Java}},
  journal       = {Future Generation Computer Systems},
  year          = {2003},
  volume        = {19},
  pages         = {611--626},
  number        = {5},
  month         = July,
  publisher     = {Elsevier},
  optcomentario = {Referencia para Lithium},
  abstract      = { In this work we present Lithium, a pure Java structured
                  parallel programming environment based on skeletons
                  (common, reusable and efficient parallelism exploitation
                  patterns). Lithium is implemented as a Java package and
                  represents both the first skeleton based programming
                  environment in Java and the first complete skeleton based
                  Java environment exploiting macro-data flow implementation
                  techniques. Lithium supports a set of user code
                  optimizations which are based on skeleton rewriting
                  techniques. These optimizations improve both absolute
                  performance and resource usage with respect to original
                  user code. Parallel programs developed using the library
                  run on any network of workstations provided the
                  workstations support plain JRE. The paper describes the
                  library implementation, outlines the optimization
                  techniques used and eventually presents the performance
                  results obtained on both synthetic and real applications.}
}

